='row'>\r\n\r\n                    {isFetching ? <div className='col-md-8 col-sm mx-auto pt-2'>\r\n                        <Skeleton count={4} />\r\n                    </div> : <> {details.length ? <div className='col-md-8 col-sm mx-auto pt-2'>\r\n                        <div class=\"container  px-2\">\r\n                            <small style={{ color: '#0D826E' }}><strong>Withdraw History</strong></small>\r\n                            <div class=\"table-responsive mt-2\">\r\n                                <table class=\"table table-responsive table-borderless\">\r\n                                    <thead>\r\n                                        <tr class=\"bg-light\">\r\n                                            <th scope=\"col\" width=\"5%\">#</th>\r\n                                            <th scope=\"col\" width=\"20%\"><small>Reference</small></th>\r\n                                            <th scope=\"col\" width=\"20%\"><small>Amount</small></th>\r\n                                            <th scope=\"col\" width=\"20%\"><small>Status</small></th>\r\n                                            <th scope=\"col\" width=\"10%\"><small>Date</small></th>\r\n\r\n                                        </tr>\r\n                                    </thead>\r\n                                    {details.map((item, index) => {\r\n                                        return (\r\n                                            <>\r\n                                                <tbody key={index}>\r\n                                                    <tr>\r\n                                                        <td><small>{index + 1}</small></td>\r\n                                                        <td><small>{item.reference}</small></td>\r\n                                                        <td><small>{item.amount_in_kobo / 100}</small></td>\r\n                                                        <td><small>{item.status}</small></td>\r\n                                                        <td><small><strong><Moment fromNow>{item.created_at}</Moment></strong></small></td>\r\n\r\n                                                    </tr>\r\n                                                </tbody>\r\n                                            </>\r\n                                        )\r\n                                    })}\r\n                                </table>\r\n                            </div>\r\n                        </div>\r\n                    </div> : <div className='col-md-6 col-sm mx-auto text-center'>\r\n                        <h5>You have no withdrawal history</h5>\r\n                        <i class=\"fad fa-shopping-cart fa-3x text-danger\"></i>\r\n                        <Link to='/dashboard/create-a-course'><button class=\"btn d-flex mx-auto\" type=\"submit\" ><b>Create a Course</b></button></Link>\r\n                    </div>} </>}\r\n\r\n                </div>\r\n\r\n            </section>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default WithdrawHistory\r\n\r\n\r\n\r\n\r\n","import axios from 'axios'\r\nimport React, { useState, useEffect } from 'react'\r\nimport Skeleton from 'react-loading-skeleton'\r\nimport { Baseurl } from '../../baseurl'\r\nimport { useHistory, useLocation } from 'react-router'\r\n\r\n\r\n\r\nconst Purchases = () => {\r\n    document.title = 'Fromhome | Purchase History'\r\n    const history = useHistory();\r\n    let path = useLocation()\r\n    const [details, setDetails] = useState([])\r\n    const [isFetching, setFecthing] = useState(false)\r\n\r\n    useEffect(() => {\r\n        setFecthing(true)\r\n        let token = sessionStorage.Token\r\n        axios.get(`${Baseurl}transactions`, {\r\n            headers: {\r\n                'Authorization': 'Bearer' + token\r\n            }\r\n        }).then((res) => {\r\n            setFecthing(false)\r\n            if (res.data) {\r\n                setDetails(res.data)\r\n            }\r\n        }).catch((error) => {\r\n            setFecthing(false)\r\n            if (error.response.status === 500) {\r\n                sessionStorage.setItem('Path', path.pathname)\r\n            }\r\n        })\r\n\r\n    }, [])\r\n    return (\r\n        <>\r\n            <section className=\"about container-fluid mt-4\" id=\"about\">\r\n                <div className='row'>\r\n\r\n                    {isFetching ? <div className='col-md-8 col-sm mx-auto pt-2'>\r\n                        <Skeleton count={4} />\r\n                    </div> : <> {details.length ? <div className='col-md-8 col-sm mx-auto pt-2'>\r\n                        <div class=\"container  px-2\">\r\n                            <small style={{ color: '#0D826E' }}><strong>Purchase History</strong></small>\r\n                            <div class=\"table-responsive mt-2\">\r\n                                <table class=\"table table-responsive table-borderless\">\r\n                                    <thead>\r\n                                        <tr class=\"bg-light\">\r\n                                            <th scope=\"col\" width=\"5%\">#</th>\r\n                                            <th scope=\"col\" width=\"20%\"><small>Category</small></th>\r\n                                            <th scope=\"col\" width=\"10%\"><small>Course</small></th>\r\n                                            <th scope=\"col\" width=\"20%\"><small>Amount</small></th>\r\n                                            <th scope=\"col\" width=\"20%\"><small>Status</small></th>\r\n\r\n                                        </tr>\r\n                                    </thead>\r\n                                    {details.map((item, index) => {\r\n                                        return (\r\n                                            <>\r\n                                                <tbody key={index}>\r\n                                                    <tr>\r\n     