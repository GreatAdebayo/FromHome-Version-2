                            onSubmit={(values, { setSubmitting }) => {\r\n                                    setShowloader(true)\r\n                                    setTimeout(() => {\r\n\r\n                                        toast(\" Course Details Saved!\", {\r\n                                            autoClose: 2000,\r\n                                        })\r\n                                        setTimeout(() => history.push('/dashboard/create-a-course/sections'), 2000)\r\n\r\n                                        setSubmitting(false);\r\n                                        setShowloader(false)\r\n                                    }, 400);\r\n                                }}\r\n                            >\r\n                                {({ isSubmitting }) => (\r\n\r\n                                    <Form>\r\n\r\n                                        <div class=\"row-1\">\r\n                                            <div class=\"row row-2\"> <span id=\"login-card-inner\">Category</span> </div>\r\n                                            <div class=\"row row-2\">\r\n                                                <Field as=\"select\" name=\"category\" class=\"form-select form-select-sm mt-1\" aria-label=\".form-select-sm example\">\r\n                                                    <option selected>Choose a category</option>\r\n                                                    <option value=\"Design\">Design</option>\r\n                                                    <option value=\"Marketing\">Marketing</option>\r\n                                                    <option value=\"IT and Software\">IT and Software</option>\r\n                                                    <option value=\"Photograpy\">Photograpy</option>\r\n                                                    <option value=\"Engineering\">Engineering</option>\r\n                                                    <option value=\"Engine