ploadCourse}><b>{showloader ? <i class=\"fas fa-spinner fa-spin\"></i> : ''} Upload Course and Proceed</b></button>\r\n                            <small className='text-danger'>*Cross-check before you upload course</small><br />\r\n\r\n                            <Link to='/dashboard/create-a-course/basic-details' style={{ cursor: 'pointer' }}><strong><i class=\"fal fa-long-arrow-left\" style={{ color: '#0D826E' }}></i> Edit Previous Stage </strong></Link>\r\n\r\n                        </div>\r\n                    </div>\r\n\r\n                </div>\r\n            </section>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Sections","import React from 'react'\r\nimport {Link} from 'react-router-dom';\r\nimport Footer from '../../landing_page/footer';\r\n\r\n\r\n\r\nconst CreateAcourse = () => {\r\n    \r\n    return (\r\n        <>\r\n            <section className=\"about container-fluid\" id=\"about\">\r\n\r\n                <div class=\"row\">\r\n                    <div className='col-md-6 mt-5 col-sm mx-auto text-center'>\r\n                        <h6>Hi Tutor, Thank your for your interest in teaching with us!</h6>\r\n                        <h5 className='mt-3'>Create a Course</h5>\r\n                        <i class=\"fad fa-books text-danger fa-3x\"></i>\r\n                        <Link to='/dashboard/create-a-course/basic-details' class=\"btn d-flex mx-auto mb-2\" type=\"submit\"><b>Proceed</b></Link>\r\n                        <small style={{ cursor: 'pointer' }} data-bs-toggle=\"modal\" data-bs-target=\"#staticBackdrop\"><strong>Read more about creating a course <i class=\"fal fa-long-arrow-right\" style={{ color: '#071B4D' }}></i></strong></small>\r\n                    </div>\r\n\r\n                </div>\r\n               \r\n            </section>\r\n\r\n           \r\n\r\n            {/* Details about creating a course */}\r\n            <div class=\"modal fade\" id=\"staticBackdrop\" data-bs-backdrop=\"static\" data-bs-keyboard=\"false\" tabi