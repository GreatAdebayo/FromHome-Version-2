ton from 'react-loading-skeleton';\r\n\r\n\r\nconst CreatedCourses = () => {\r\n    document.title = 'Fromhome | Created Courses'\r\n    const history = useHistory();\r\n    const [course, setCourse] = useState([])\r\n    const path = useLocation();\r\n    const [isFetching, setFecthing] = useState(false)\r\n\r\n    useEffect(() => {\r\n        setFecthing(true)\r\n        let token = sessionStorage.Token\r\n        if (token) {\r\n            axios.get(`${Baseurl}createdcourse`, {\r\n                params: {\r\n                    token: token\r\n                }\r\n            }).then((res) => {\r\n                setFecthing(false)\r\n                if (res.data) {\r\n                    setCourse(res.data)\r\n                }\r\n            }).catch((error) => {\r\n                setFecthing(false)\r\n                if (error.response.status === 500) {\r\n                    sessionStorage.setItem('Path', path.pathname)\r\n                }\r\n            })\r\n        } else {\r\n\r\n        }\r\n\r\n    }, [])\r\n\r\n    const courseStatus = (code) => {\r\n        history.push(`/dashboard/created-courses/manage/edit-basic-details/${code}`)\r\n        axios.post(`${Baseurl}coursestatus`, { code: code }).then((res) => {\r\n\r\n        })\r\n    }\r\n    return (\r\n        <>\r\n            <div className='container mb-3'>\r\n                <div className='row' style={{ marginTop: '90px' }}>\r\n                    <div className='col-md-6 col-sm mx-auto text-center'>\r\n                        <h5>Your have {course.length ? course.length : 'no'} course{course.length > 1 ? 's' : ''}</h5>\r\n                        <i class=\"fas fa-books text-danger fa-3x\"></i>\r\n                        <button class=\"btn d-flex mx-auto\" onClick={() => history.push('/dashboard/create-a-course')} ><b><small>Create a new course</small></b></button>\r\n                    </div>\r\n\r\n                </div>\r\n\r\n            </div>\r\n            <div className='contain