er-dom\";\r\n\r\nconst DashboardGuard = ({\r\n  component: Component,\r\n  auth,\r\n  ...theRemainingProps\r\n}) => {\r\n  return (\r\n    <Route\r\n      {...theRemainingProps}\r\n      render={() => {\r\n        return auth ? <Component /> : <Redirect to=\"/login\" />;\r\n      }}\r\n    ></Route>\r\n  );\r\n};\r\n\r\nexport default DashboardGuard;","import axios from 'axios'\r\nimport React, { useEffect, useState } from 'react'\r\nimport { Baseurl } from '../baseurl'\r\nimport Skeleton from 'react-loading-skeleton'\r\nimport { Link } from 'react-router-dom'\r\n\r\n\r\n\r\nconst  RelatedCourses = ({code}) => {\r\n    const [course, setCourse] = useState([])\r\n    const [isFetching, setFecthing] = useState(false)\r\n    useEffect(() => {\r\n        setFecthing(true)\r\n        axios.get(`${Baseurl}relatedcourses`, {\r\n            params:{code:code}\r\n        }).then((res) => {\r\n            setFecthing(false)\r\n            if (res.data) {\r\n                setCourse(res.data)\r\n                \r\n            }\r\n        })\r\n    }, [])\r\n    return (\r\n        <>\r\n            <div class=\"container mb-4\">\r\n                <div class=\"row\">\r\n                    <small style={{color:'#0D826E'}}><strong> Related Courses</strong></small>\r\n                    {isFetching ? <div className='row mx-auto'>\r\n                        <div class=\"col-md-4 mt-2\">\r\n                            <div class=\"card p-3 mb-2 shadow\" style={{ borderRadius: '20px' }} >\r\n                                <Skeleton count={5} />\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-md-4 mt-2\">\r\n                            <div class=\"card p-3 mb-2 shadow\" style={{ borderRadius: '20px' }} >\r\n                                <Skeleton count={5} />\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-md-4 mt-2\">\r\n                   